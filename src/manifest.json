{
    "manifest_version": 2,
    "name": "Roll20 Pathfinder 2e Creature Importer",
    "version": "0.1",
  
    "description": "Imports Foundry PF2 Creatures into Roll20",
    "browser_action": {
      "default_title": "Import creature",
      "default_popup": "/index.html"
    }, 
    "content_scripts": [
      {
        "matches": ["https://app.roll20.net/editor/"],
        "js": ["assets/js/jquery-3.6.3.min.js"]
      }
    ],
    "permissions": [
      "activeTab", "storage"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "aonexporter@cwust.br"
      }
    }
  
  }
  